<h1>Login with SiteGod Privileges</h1>
<FORM name="LoginForm" enctype="multipart/form-data" ACTION="CS.Property.ft.approotContentServer?pagename=Support/Info/collectInfo" METHOD="POST" REPLACEALL="CS.Property.ft.approot">
<INPUT TYPE="HIDDEN" NAME="login" VALUE="true"/>
<TABLE style="border:none">
    <TR> 
      <TD style="text-align:right; border:none"><b>User ID: </b></TD>
      <TD style="border:none"><INPUT TYPE="TEXT" NAME="username" size="20"/></TD>
    </TR>
    <TR> 
      <TD style="text-align:right; border:none"><b>Password: </b></TD>
      <TD style="border:none"><INPUT TYPE="PASSWORD" NAME="password" size="20"/></TD>
    </TR>
    <TR> 
      <TD style="border:none">&nbsp;</TD>
      <TD style="border:none"><INPUT TYPE="submit" VALUE="Login"/>&nbsp;<INPUT TYPE="reset" VALUE="Cancel"/></TD>
    </TR>
  </TABLE>
</FORM>
<script language="JavaScript">
<![CDATA[
    document.LoginForm.username.focus();
]]>
</script>
</ftcs>
